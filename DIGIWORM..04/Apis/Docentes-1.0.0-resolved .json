{
  "swagger": "2.0",
  "info": {
    "description": "The API for digiworm",
    "version": "1.0.0",
    "title": "Docentes"
  },
  "host": "virtserver.swaggerhub.com",
  "basePath": "/VILLABILONS/Docentes/1.0.0",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/devices": {
      "get": {
        "tags": [
          "Device"
        ],
        "operationId": "getDevices",
        "parameters": [
          {
            "name": "skip",
            "in": "query",
            "description": "Number of records to skip",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Max number of records to return",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "All the devices",
            "examples": {
              "application/json": [
                {
                  "Nombres": "juan david",
                  "Apellidos": "julio rodriguez",
                  "img": "img/img_6614b7979115d_c89f135f391170c34a8599024277be5d.jpg",
                  "Materia": 2,
                  "nombre_materia": "Espa√±ol"
                }
              ]
            },
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Device"
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Device"
        ],
        "operationId": "register",
        "parameters": [
          {
            "in": "body",
            "name": "Curso",
            "required": false,
            "schema": {
              "$ref": "#/definitions/DeviceRegistrationInfo"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successfully registered device"
          }
        }
      }
    }
  },
  "definitions": {
    "Device": {
      "type": "object",
      "properties": {
        "Nombres": {
          "type": "string"
        },
        "Apellidos": {
          "type": "string"
        },
        "img": {
          "type": "string"
        },
        "Materia": {
          "type": "integer"
        },
        "nombre_materia": {
          "type": "string"
        }
      }
    },
    "DeviceRegistrationInfo": {
      "type": "object",
      "properties": {
        "uri": {
          "type": "string",
          "format": "uri",
          "example": "http://localhost/PROYECTO/DIGIWORM..04/apis/DocentesApis.php"
        },
        "id": {
          "type": "string",
          "format": "uuid",
          "example": "0729a580-2240-11e6-9eb5-0002a5d5c51b"
        }
      }
    }
  }
}